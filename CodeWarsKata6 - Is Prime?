function isPrime(num) {
  //TODO
 var array = [];
 
 for (i=0; i<=num; i++){
 	if (num%i === 0) {
 		array.push(i);
 	}}
 	
 	if (array[0] === 1 && array[1] === num) {
 		return true;}
    else {return false;}
}

isPrime(23)
